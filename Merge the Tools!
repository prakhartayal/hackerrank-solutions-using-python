from collections import OrderedDict
def merge_the_tools(string, k):
    
    lens=len(string)

    for i in range(0,lens,k):
        t=string[i:i+k]
        print("".join(OrderedDict.fromkeys(t)))
    
   
    


  
if __name__ == '__main__':
    string, k = input(), int(input())
    merge_the_tools(string, k)
